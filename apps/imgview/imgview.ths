# This is the theme loaded by imgview, in imgview.th.h
# This should be included as a resource when pgres is done!

object "imgview.box" {
  margin = 0;
  bitmap1 = LoadBitmap("checkers.bmp");
  bgfill = fillstyle {
    Bitmap(x,y,w,h,bitmap1);
  };	
}	

object "imgview.panel" {
  margin = 0;
  bgfill = fillstyle {
    Nop();
  };
}

 	

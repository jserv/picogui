/* $Id: mod_dimmer.ths,v 1.1 2002/02/24 17:52:47 micahjd Exp $
 *
 * Dim backgrounds behind popups
 *
 * -- Micah Dowty
 */

object default name = "Modifier: Popup background dimmer";

object popup backdrop = fillstyle {
  SetLgop(PG_LGOP_ALPHA);
  SetColor(PGCF_ALPHA | PGC_WHITE | (70<<24));
  Rect(0,0,0x7FFF,0x7FFF);
};

/* The End */

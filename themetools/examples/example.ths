/* This is an example
   theme file */

obj default {
  bgfill = fillstyle {
    Frame(x,y,w,h,fgcolor);
    x = x+1;
    y = y+1;
    w = w-2;
    h = h-2;
    Rect(x,y,w,h,default:bgcolor);
  };
  fgcolor = 0x000000;
  bgcolor = 0xFFFFFF;
};

obj button.on bgcolor = 5;
object themeinfo name = "Example\n\"theme\"";

object button {
    font = 0xFFFFFF;
    fgcolor = 0x000050 + 16000;
    name = 0x394230;
    bgcolor = 0x29;
};
